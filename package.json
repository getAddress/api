{
  "name": "getaddress-api",
  "version": "1.2.4",
  "description": "GetAddress.io core address look-up functionality",
  "types": "./dist/getaddress-api.d.ts",
  "main": "./dist/getaddress-api.mjs",
  "engines": {
    "node": ">= 13.2.0"
  },
  "homepage": "https://getaddress.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/getAddress/api"
  },
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js -c",
    "cleanDist": "rimraf dist/*",
    "rollup": "rollup -c",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "npm-run-all cleanDist rollup test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "getaddress.io postcode getaddress getaddress() find autocomplete"
  ],
  "author": "Lee Smith",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    }
  },
  "dependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "eslint-config-standard": "^17.0.0",
    "isomorphic-fetch": "^3.0.0"
  }
}
