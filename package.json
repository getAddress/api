{
  "name": "getaddress-api",
  "version": "1.1.8",
  "description": "GetAddress.io core address look-up functionality",
  "types": "./@types/getaddress-api.d.ts",
  "main": "./dist/getaddress-api.mjs",
  "engines": {
    "node": ">= 13.2.0"
  },
  "homepage": "https://github.com/getAddress/api/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/getAddress/api"
  },
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js -c",
    "cleanDist": "rimraf dist/*",
    "cleanSrc": "rimraf src/*",
    "cleanTypes": "rimraf ./@types/*.d.ts",
    "compile": "tsc --build tsconfig.json",
    "rollup": "rollup -c",
    "build": "npm-run-all cleanDist cleanSrc cleanTypes compile rollup test",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "getaddress.io postcode getaddress getaddress() find autocomplete"
  ],
  "author": "Lee Smith",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.3",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    }
  },
  "dependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "isomorphic-fetch": "^3.0.0"
  }
}
